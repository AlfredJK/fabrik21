<?xml version="1.0" encoding="UTF-8"?>
<install version="1.5" type="fabrikplugin" group="element" method="upgrade">
	<name>google o meter</name>
	<author>Rob Clayburn</author>
	<creationDate>July 2009</creationDate>
	<copyright>(C) 2005-2009 fabrikar.com</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<authorEmail>rob@pollen-8.co.uk</authorEmail>
	<authorUrl>www.fabrikar.com</authorUrl>
	<version>2.1</version>
	<description>Renders google o meter viz</description>
	<files>
		<filename fabrikplugin="fabrikgoogleometer">fabrikgoogleometer.php</filename>
		<filename>index.html</filename>
	</files>
	
	<administration>
	  <languages folder="administrator/language" >
			<language tag="de-DE">de-DE/de-DE.com_fabrik.plg.element.fabrikgoogleometer.ini</language>
			<language tag="en-GB">en-GB/en-GB.com_fabrik.plg.element.fabrikgoogleometer.ini</language>
			<language tag="es-ES">es-ES/es-ES.com_fabrik.plg.element.fabrikgoogleometer.ini</language>
			<language tag="fr-FR">fr-FR/fr-FR.com_fabrik.plg.element.fabrikgoogleometer.ini</language>
			<language tag="it-IT">it-IT/it-IT.com_fabrik.plg.element.fabrikgoogleometer.ini</language>
		</languages>
	</administration>
	
	<params>
		<param name="googleometer_element" type="tablefields" label="GOOGLEOMETER_ELEMENT" description="GOOGLEOMETER_ELEMENT_DESC"/>
		<param name="googleometer_width" type="text" size="3" default="200" label="GOOGLEOMETER_WIDTH" description="GOOGLEOMETER_WIDTH_DESC" />
		<param name="googleometer_height" type="text" size="3" default="125" label="GOOGLEOMETER_HEIGHT" description="GOOGLEOMETER_HEIGHT_DESC" />
		<param name="googleometer_label" type="text" size="20" default="" label="GOOGLEOMETER_LABEL" description="GOOGLEOMETER_LABEL_DESC" />
	</params>
</install>